<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearLayout1"
    android:layout_width="315dp"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:background="#fff" >

    <LinearLayout
        android:id="@+id/llBackOperation"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="5dp">

        <TextView
            android:id="@+id/txtTitle"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/logo1" />

        <TextView
            android:id="@+id/txtMessage"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            android:gravity="center"
            android:maxLines="10"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="15dp"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <Button
                android:id="@+id/btnOk"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/csv_file"
                android:textColor="#fff"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btnCancel"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/address_book"
                android:textColor="#fff"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btnGmail_importing"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/gmail"
                android:textColor="#fff"
                android:textStyle="bold" />


            <!--denisha-->
            <Button
                android:visibility="gone"
                android:id="@+id/btnYahoo_importing"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/yahoo"
                android:textColor="#fff"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btnAweber_importing"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/aweber"
                android:textColor="#fff"
                android:textStyle="bold" />
            <Button
                android:visibility="gone"
                android:id="@+id/btnhubsport_export"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/strExportHubSport"
                android:textColor="#fff"
                android:textStyle="bold" />
            <Button
                android:id="@+id/btnSalesForce_importing"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/salesforce"
                android:textColor="#fff"
                android:textStyle="bold"
                android:visibility="gone" />
            <Button
                android:id="@+id/btnHubspot_import"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/strimporthubspot"
                android:textColor="#fff"
                android:textStyle="bold"
             />
            <Button
                android:id="@+id/btnCancel_importing"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.8"
                android:background="@drawable/neweightattachmentbtn"
                android:text="@string/cancel"
                android:textColor="#fff"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>