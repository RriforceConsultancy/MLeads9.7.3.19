<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#fff"
    android:orientation="vertical" >

    <include 
        layout="@layout/commonlayout"
        android:id="@+id/commonlayout"
        />

  <!--   <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1" > -->

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:id="@+id/llMain"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginBottom="60dp"
            android:gravity="top"
            android:orientation="vertical"
            android:weightSum="4" >

            <LinearLayout
                android:id="@+id/l2"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_gravity="right|center"
                android:background="@drawable/neweightfooter"
                android:gravity="center" >

                <TextView
                    android:id="@+id/TextHome"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|center"
                    android:gravity="center"
                    android:text="@string/home_label"
                    android:textColor="#ffffff"
                    android:textSize="20dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <FrameLayout
                android:id="@+id/EventsButton"
                android:layout_width="fill_parent"
                android:layout_height="90dp"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/neweighthomebg"
                android:onClick="onClick" >

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="60dp"
                    android:layout_height="55dp"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/neweightevents" />

                <TextView
                    android:id="@+id/txt_event"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="70dp"
                    android:text="@string/EventsLabel"
                    android:textColor="#000000"
                    android:textSize="19dip" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/FollowUpMessagesButton"
                android:layout_width="fill_parent"
                android:layout_height="90dp"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/neweighthomebg"
                android:onClick="onClick" >

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/neweightemailhome" />

                <TextView
                    android:id="@+id/txt_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="70dp"
                    android:text="@string/email_template"
                    android:textColor="#000000"
                    android:textSize="18dip" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/AddLeadsButton"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="left|center"
                    android:background="@drawable/newaddlead" />

                <TextView
                    android:id="@+id/txt_addlead"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/add_lead"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/MyLeadsButton"
                android:layout_width="fill_parent"
                android:layout_height="90dp"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/neweighthomebg"
                android:onClick="onClick" >

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="60dp"
                    android:layout_height="55dp"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/neweightmleads" />

                <TextView
                    android:id="@+id/txt_lead"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="70dp"
                    android:text="@string/MyLeadsLabel"
                    android:textColor="#000000"
                    android:textSize="19dip" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/ReportsButton"
                android:layout_width="fill_parent"
                android:layout_height="90dp"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/neweighthomebg"
                android:onClick="onClick" >

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="60dp"
                    android:layout_height="55dp"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/neweightreport" />

                <TextView
                    android:id="@+id/txt_report"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="70dp"
                    android:text="@string/reportstitle"
                    android:textColor="#000000"
                    android:textSize="19dip" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/AccountButton"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="left|center"
                    android:background="@drawable/accounts" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/accountpage"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/FollowUpActionButton"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/action_home" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/FollowUpActionLabel"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/myvideoprofile"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView41"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/videoprofile2" />

                <TextView
                    android:id="@+id/textView41"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/myvideoprofile"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView151"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/myqrcode"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView42"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/myqrcode12" />

                <TextView
                    android:id="@+id/textView42"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/myqrcode"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView152"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/SynchronizeToServerButton"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/sync_home" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/SynchronizeToServerLabel"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView19"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/MyProfileButton"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView61"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/profile_home" />

                <TextView
                    android:id="@+id/textView61"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/myProfileTitle"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView17"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/PasswordResetButton"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/reset_home" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/PasswordResetLabel"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView18"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/ReferFriendButton"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/imageView43"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/refer_home" />

                <TextView
                    android:id="@+id/textView43"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:text="@string/ReferFriendLabel"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/MyLeadsonWeb"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:background="@drawable/middle_home_xml"
                android:onClick="onClick"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/ImageView01"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center|left"
                    android:background="@drawable/manage_home" />

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="60dp"
                    android:gravity="center"
                    android:text="@string/MyLeadsOnWeb"
                    android:textColor="#000000"
                    android:textSize="16dip" />

                <ImageView
                    android:id="@+id/imageView16"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right|center"
                    android:background="@drawable/right_home_arrow" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:layout_gravity="bottom"
            android:background="@drawable/neweightfooter"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/btnSettings"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal|center_vertical"
                android:background="@drawable/neweightfooter_logo" />
        </FrameLayout>

    </FrameLayout>

</LinearLayout>