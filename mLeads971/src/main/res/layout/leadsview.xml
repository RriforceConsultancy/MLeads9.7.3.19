<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="fill_parent"    android:layout_height="fill_parent" >	<android.webkit.WebView android:layout_width="fill_parent"		android:layout_height="fill_parent" android:id="@+id/web"		android:visibility="gone"></android.webkit.WebView>    <LinearLayout        android:id="@+id/llMain"        android:layout_width="fill_parent"        android:layout_height="fill_parent"        android:background="@drawable/main_background"        android:orientation="vertical" >      <!--   <FrameLayout            android:id="@+id/linearAddview"            android:layout_width="fill_parent"            android:layout_height="50dp" >        </FrameLayout> -->        <FrameLayout            android:layout_width="fill_parent"            android:layout_height="50dp"            android:layout_gravity="center"            android:background="@drawable/title_bar" >            <Button                android:id="@+id/BackButton"                android:layout_width="70dip"                android:layout_height="30dip"                android:layout_gravity="center|left"                android:layout_marginLeft="2dip"                android:background="@drawable/back"                android:text="@string/BackButtonLabel"                android:textColor="#FFFFFF" />            <TextView                android:id="@+id/textView1"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_toRightOf="@+id/BackButton"                android:gravity="center"                android:text="@string/viewleadspagetitle"                android:textColor="#000000"                android:textSize="17dp"                android:textStyle="bold" >            </TextView>        </FrameLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:gravity="center" >            <TextView                android:layout_width="100dp"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginLeft="20dp"                android:gravity="left"                android:text="@string/FirstNameLabel"                android:textColor="#000000" />            <TextView                android:id="@+id/FirstNameEditText"                android:layout_width="fill_parent"                android:layout_height="20dip"                android:layout_marginRight="7dp"                android:focusable="false"                android:maxLength="50"                android:singleLine="true"                android:textColor="#000000" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:gravity="center" >            <TextView                android:layout_width="100dp"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginLeft="20dp"                android:gravity="left"                android:text="@string/LastNameLabel"                android:textColor="#000000" />            <TextView                android:id="@+id/LastNameEditText"                android:layout_width="fill_parent"                android:layout_height="20dip"                android:layout_marginRight="7dp"                android:focusable="false"                android:maxLength="50"                android:textColor="#000000"                android:singleLine="true" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:gravity="center" >            <TextView                android:layout_width="100dp"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginLeft="20dp"                android:gravity="left"                android:text="@string/CompanyNameLabel"                android:textColor="#000000" />            <TextView                android:id="@+id/CompanyNameEditText"                android:layout_width="fill_parent"                android:layout_height="20dip"                android:layout_marginRight="7dp"                android:focusable="false"                android:maxLength="50"                android:textColor="#000000"                android:singleLine="true" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:gravity="center" >            <TextView                android:layout_width="100dp"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginLeft="20dp"                android:gravity="left"                android:text="@string/AddLeadJobTitleLable"                android:textColor="#000000" />            <TextView                android:id="@+id/JobTitleEditText"                android:layout_width="fill_parent"                android:layout_height="20dip"                android:layout_marginRight="7dp"                android:focusable="false"                android:maxLength="50"                android:textColor="#000000"                android:singleLine="true" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:gravity="center" >            <TextView                android:layout_width="100dp"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginLeft="20dp"                android:gravity="left"                android:text="@string/EmailLabel"                android:textColor="#000000" />            <TextView                android:id="@+id/EmailEditText"                android:layout_width="fill_parent"                android:layout_height="20dip"                android:layout_marginRight="7dp"                android:focusable="false"                android:textColor="#000000"                android:maxLength="50"                android:singleLine="true" />        </LinearLayout>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:gravity="center" >            <TextView                android:layout_width="100dp"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginLeft="20dp"                android:gravity="left"                android:text="@string/AddLeadPhoneLabel"                android:textColor="#000000" />            <TextView                android:id="@+id/PhoneEditText"                android:layout_width="fill_parent"                android:layout_height="20dip"                android:layout_marginRight="7dp"                android:focusable="false"                android:textColor="#000000"                android:maxLength="50"                android:singleLine="true" />        </LinearLayout>        <ScrollView            android:layout_width="fill_parent"            android:layout_height="fill_parent"            android:layout_marginTop="2dp" >            <LinearLayout                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:layout_marginTop="1dp"                android:gravity="center"                android:orientation="vertical" >                <LinearLayout                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp" >                    <TextView                        android:layout_width="180dp"                        android:layout_height="wrap_content"                        android:layout_gravity="center|left"                        android:layout_marginLeft="20dp"                        android:text="@string/viewpagetitleleadaction"                        android:textColor="#000000"                        android:textSize="18dp" />                </LinearLayout>                <ImageView                    android:layout_width="fill_parent"                    android:layout_height="10dp"                    android:layout_gravity="center|left"                    android:src="@drawable/grayline2" />                <LinearLayout                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="2dp"                    android:background="@drawable/title_bar"                    android:weightSum="4" >                    <ImageView                        android:id="@+id/viewLeadImageView"                        android:layout_width="0dp"                        android:layout_height="50dp"                        android:layout_gravity="center|left"                        android:layout_weight="1"                        android:background="@drawable/view" />                                        <ImageView                        android:id="@+id/edit"                        android:layout_width="0dp"                        android:layout_height="53dp"                        android:layout_gravity="center|left"                        android:layout_weight="1"                        android:src="@drawable/newedit" />                    <ImageView                        android:id="@+id/delete"                        android:layout_width="0dp"                        android:layout_height="53dp"                        android:layout_weight="1"                        android:background="@drawable/newdelete" />                                        <ImageView                        android:id="@+id/email"                        android:layout_width="0dp"                        android:layout_height="53dp"                        android:layout_weight="1"                        android:background="@drawable/newemail" />                                                            <ImageView                        android:id="@+id/call"                        android:layout_width="0dp"                        android:layout_height="53dp"                        android:layout_gravity="center|left"                        android:layout_marginLeft="3dp"                        android:layout_weight="1"                        android:src="@drawable/newcall"                         />                                                       </LinearLayout>                <LinearLayout                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="4dp"                    android:background="@drawable/title_bar"                    android:weightSum="4" >                    <ImageView                        android:id="@+id/sms"                        android:layout_width="0dp"                        android:layout_height="53dp"                        android:layout_gravity="center|left"                        android:layout_marginLeft="4dp"                        android:layout_weight="1"                        android:src="@drawable/newsms" />                                                                                <ImageView                        android:id="@+id/task"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:src="@drawable/newtask" />                    <ImageView                        android:id="@+id/meeting"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:src="@drawable/newmeeting" />                                         <ImageView                        android:id="@+id/addaddress"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_gravity="center|left"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:src="@drawable/newaddaddress" />                                                         </LinearLayout>                <LinearLayout                    android:id="@+id/oneclicksearch"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="4dp" >                    <TextView                        android:layout_width="180dp"                        android:layout_height="wrap_content"                        android:layout_gravity="center|left"                        android:layout_marginLeft="20dp"                        android:text="@string/viewpagetitleleadsoneclicksearch"                        android:textColor="#000000"                        android:textSize="18dp" />                </LinearLayout>                <ImageView                    android:id="@+id/oneclickimage"                    android:layout_width="fill_parent"                    android:layout_height="10dp"                    android:layout_gravity="center|left"                    android:src="@drawable/grayline2" />                <LinearLayout                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="2dp"                    android:background="@drawable/title_bar"                    android:weightSum="5"                     android:id="@+id/graybar_two"                    >                    <ImageView                        android:id="@+id/linkedin"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:src="@drawable/newlinkedin" />                    <ImageView                        android:id="@+id/profile"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:src="@drawable/newprofile" />                    <ImageView                        android:id="@+id/gsearch"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:src="@drawable/newgsearch" />                    <ImageView                        android:id="@+id/maps"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:src="@drawable/newmaps" />                    <ImageView                        android:id="@+id/web23"                        android:layout_width="0dp"                        android:layout_height="57dp"                        android:layout_marginLeft="2dp"                        android:layout_weight="1"                        android:background="@drawable/newweb" />                </LinearLayout>            </LinearLayout>        </ScrollView>    </LinearLayout></LinearLayout>